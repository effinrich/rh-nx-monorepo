    "affected": "nx affected",
    "format": "nx format --dry-run",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest && npm install --legacy-peer-deps && nx migrate --run-migrations=migrations.json && npm run format:write && npm run test",
    "chromatic": "npx chromatic --only-changed --exit-zero-on-changes --exit-once-uploaded --debug --trace-changed=expanded",
    "graph": "nx graph",
    "reset": "nx reset",
    "repair": "nx repair",
    "help": "nx help",
    "storybook": "nx run shared-ui:storybook",
    "build-storybook": "nx run portal-ui:build-storybook",
    "test-storybook:shared-ui": "nx run shared-ui:test-storybook",
    "test-storybook:portal-ui": "nx run portal-ui:test-storybook",
    "storybook-portal-ui": "nx run portal-ui:storybook",
    "build-storybook-portal-ui": "nx run portal-ui:build-storybook",
    "storybook-all": "nx run-many --targets=storybook --projects=portal-ui,shared-ui",
    "theme": "chakra-cli tokens ./libs/shared/ui/src/lib/theme/index.ts",
    "theme:watch": "chakra-cli tokens ./libs/shared/ui/src/lib/theme/index.ts --watch",
    "mcp:storybook:install": "cd tools/storybook-mcp && npm install",
    "mcp:storybook:build": "cd tools/storybook-mcp && npm run build",
    "mcp:storybook:dev": "cd tools/storybook-mcp && npm run dev",
    "mcp:setup": "npm run mcp:storybook:install && npm run mcp:storybook:build"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
