    "start": "nx serve",
    "build": "nx build",
    "start:portal": "nx run portal:serve",
    "build:portal": "nx run portal:build",
    "lint:portal": "nx run portal:lint --fix",
    "test:portal": "nx run portal:test",
    "build:ui": "nx run shared-ui:build",
    "start:parser-playground": "nx run parser-playground:serve",
    "test": "nx test",
    "lint": "nx run-many --target=lint --all --fix",
    "check-types:all": "nx run-many --target=check-types --all",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test --verbose",
    "affected:lint": "nx affected:lint",
    "affected:check-types": "nx affected:check-types",
