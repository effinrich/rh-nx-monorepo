{
  "name": "third-party-network",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/third-party-network/src",
  "projectType": "application",
  "tags": ["scope:third-party-network"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/third-party-network"
      },
      "defaultConfiguration": "dev",
      "configurations": {
        "dev": {
          "mode": "dev"
        },
        "staging": {
          "mode": "staging"
        },
        "prod": {
          "mode": "prod"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "third-party-network:build",
        "hmr": true,
        "mode": "dev"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/apps/third-party-network"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/third-party-network"
      },
      "defaultConfiguration": "default",
      "configurations": {
        "default": {
          "watch": false,
          "reporters": ["default"]
        },
        "watch": {
          "watch": true,
          "reporters": ["verbose"]
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "defaultConfiguration": "default",
      "configurations": {
        "default": {
          "fix": false
        },
        "format": {
          "fix": true
        }
      }
    },
    "build-image": {
      "dependsOn": ["build"],
      "executor": "@nx-tools/nx-container:build",
      "options": {
        "load": true,
        "platforms": ["linux/arm64"],
        "tags": ["${NX_CONTAINER_IMAGE_TAG}"]
      }
    }
  }
}
