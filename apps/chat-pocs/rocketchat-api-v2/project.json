{
  "name": "rocketchat-api-v2",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/chat-pocs/rocketchat-api-v2/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/chat-pocs/rocketchat-api-v2",
        "main": "apps/chat-pocs/rocketchat-api-v2/src/main.ts",
        "tsConfig": "apps/chat-pocs/rocketchat-api-v2/tsconfig.app.json",
        "assets": ["apps/chat-pocs/rocketchat-api-v2/src/assets"],
        "webpackConfig": "apps/chat-pocs/rocketchat-api-v2/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "rocketchat-api-v2:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "rocketchat-api-v2:build:development"
        },
        "production": {
          "buildTarget": "rocketchat-api-v2:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
