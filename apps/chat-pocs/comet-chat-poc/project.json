{
  "name": "chat-pocs-comet-chat-poc",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/chat-pocs/comet-chat-poc/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/chat-pocs/comet-chat-poc"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "chat-pocs-comet-chat-poc:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "chat-pocs-comet-chat-poc:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "chat-pocs-comet-chat-poc:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "chat-pocs-comet-chat-poc:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "chat-pocs-comet-chat-poc:build:development"
        },
        "production": {
          "buildTarget": "chat-pocs-comet-chat-poc:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/apps/chat-pocs/comet-chat-poc"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
