{
  "name": "expo-poc",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/expo-poc/src",
  "projectType": "application",
  "tags": ["scope:expo-poc"],
  "targets": {
    "start": {
      "executor": "@nx/expo:start",
      "options": {
        "port": 8081
      },
      "dependsOn": ["ensure-symlink", "sync-deps"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx start expo-poc"
      }
    },
    "run-web": {
      "executor": "nx:run-script",
      "options": {
        "script": "web"
      }
    },
    "run-ios": {
      "executor": "@nx/expo:run",
      "options": {
        "platform": "ios"
      },
      "dependsOn": ["ensure-symlink", "sync-deps"]
    },
    "run-android": {
      "executor": "@nx/expo:run",
      "options": {
        "platform": "android"
      },
      "dependsOn": ["ensure-symlink", "sync-deps"]
    },
    "build": {
      "executor": "@nx/expo:build",
      "options": {}
    },
    "build-list": {
      "executor": "@nx/expo:build-list",
      "options": {}
    },
    "download": {
      "executor": "@nx/expo:download",
      "options": {
        "output": "apps/expo-poc/dist"
      }
    },
    "build-ios": {
      "executor": "@nx/expo:build-ios",
      "options": {}
    },
    "build-android": {
      "executor": "@nx/expo:build-android",
      "options": {}
    },
    "build-web": {
      "executor": "@nx/expo:build-web",
      "options": {}
    },
    "build-status": {
      "executor": "@nx/expo:build-web",
      "options": {}
    },
    "sync-deps": {
      "executor": "@nx/expo:sync-deps",
      "options": {}
    },
    "ensure-symlink": {
      "executor": "@nx/expo:ensure-symlink",
      "options": {}
    },
    "publish": {
      "executor": "@nx/expo:publish",
      "options": {}
    },
    "publish-set": {
      "executor": "@nx/expo:publish-set",
      "options": {}
    },
    "rollback": {
      "executor": "@nx/expo:rollback",
      "options": {}
    },
    "prebuild": {
      "executor": "@nx/expo:prebuild",
      "options": {},
      "dependsOn": ["ensure-symlink", "sync-deps"]
    },
    "eject": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx prebuild expo-poc"
      }
    },
    "install": {
      "executor": "@nx/expo:install",
      "options": {}
    },
    "update": {
      "executor": "@nx/expo:update",
      "options": {}
    },
    "export": {
      "executor": "@nx/expo:export",
      "options": {
        "platform": "all",
        "outputDir": "../../dist/apps/expo-poc"
      },
      "dependsOn": ["ensure-symlink", "sync-deps"]
    },
    "export-web": {
      "executor": "@nx/expo:export",
      "options": {
        "bundler": "metro"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/expo-poc/jest.config.ts",
        "passWithNoTests": true
      },
      "defaultConfiguration": "ci",
      "configurations": {
        "ci": {
          "runInBand": true,
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
