{
  "name": "portal-features-sign-in",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/portal/features/sign-in/src",
  "projectType": "library",
  "tags": ["scope:portal"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/portal/features/sign-in/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/portal/features/sign-in",
        "tsConfig": "libs/portal/features/sign-in/tsconfig.lib.json",
        "project": "libs/portal/features/sign-in/package.json",
        "entryFile": "libs/portal/features/sign-in/src/index.ts",
        "external": ["react", "react-dom", "@emotion/react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/portal/features/sign-in/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/portal/features/sign-in/jest.config.ts",
        "passWithNoTests": true
      },
      "defaultConfiguration": "ci",
      "configurations": {
        "ci": {
          "runInBand": true,
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
