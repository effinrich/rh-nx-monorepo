    "theme": "chakra-cli tokens ./libs/shared/ui/src/lib/theme/index.ts",
    "theme:watch": "chakra-cli tokens ./libs/shared/ui/src/lib/theme/index.ts --watch"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
